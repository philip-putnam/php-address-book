<html>
<head>
    <title>Address Book</title>
</head>
<body>
    <h1>Address Book Home</h1>
    {% for contact in contacts %}
        <h3>{{ contact.getName }}</h3>
        <ul>
        <li>{{ contact.getPhoneNumber }}</li>
        <li>{{ contact.getAddress }}</li>
        </ul>
    {% endfor %}

</body>
</html>
